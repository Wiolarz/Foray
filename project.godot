config_version=5

[application]

config/name="Krong"
run/main_scene="res://Scenes/MainScene.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://Art/old/elf3.png"

[autoload]

BUS="*res://Scripts/General/bus.gd"
W_GRID="*res://Scripts/World/world_grid.gd"
WM="*res://Scripts/World/world_manager.gd"
B_GRID="*res://Scripts/Battle/battle_grid.gd"
BM="*res://Scripts/Battle/battle_manager.gd"
IM="*res://Scripts/General/input_manager.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "blue",
"res://Documentation/": "green",
"res://Resources/": "orange",
"res://Scenes/": "teal",
"res://Scripts/Battle/": "red",
"res://Scripts/General/": "red",
"res://Scripts/World/": "red",
"res://test/": "pink"
}

